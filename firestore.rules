rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    
    match /accounts/{accountId} {
      allow read: if true;
      allow create, update, delete: if request.auth != null && request.auth.uid == resource.data.uid;
    }

    match /messages/{messageId} {
      allow read: if true;
      allow create, update, delete: if request.auth != null && request.auth.uid == resource.data.uid;
    }
  }
}